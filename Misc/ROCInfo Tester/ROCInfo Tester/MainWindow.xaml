<Window x:Class="ROCInfo_Tester.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" Closed="Window_Closed">
    <Grid>
        <Label Content="CPU TEMP:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="CPU LOAD:" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top"/>
        <Label Content="GPU TEMP:" HorizontalAlignment="Left" Margin="10,62,0,0" VerticalAlignment="Top"/>
        <Label Content="GPU LOAD:" HorizontalAlignment="Left" Margin="10,88,0,0" VerticalAlignment="Top"/>
        <Label Content="RAM LOAD:" HorizontalAlignment="Left" Margin="10,114,0,0" VerticalAlignment="Top"/>
        <Label x:Name="cpuTempLabel" Content="NA" HorizontalAlignment="Left" Margin="87,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="cpuLoadLabel" Content="NA" HorizontalAlignment="Left" Margin="87,36,0,0" VerticalAlignment="Top"/>
        <Label x:Name="gpuTempLabel" Content="NA" HorizontalAlignment="Left" Margin="87,62,0,0" VerticalAlignment="Top"/>
        <Label x:Name="gpuLoadLabel" Content="NA" HorizontalAlignment="Left" Margin="87,88,0,0" VerticalAlignment="Top"/>
        <Label x:Name="ramLoadLabel" Content="NA" HorizontalAlignment="Left" Margin="87,114,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
